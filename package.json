{
  "name": "car-booking-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "babel-node src/db/seeders/index.js",
    "seed:cars": "babel-node src/db/seeders/cars.js",
    "seed:users": "babel-node src/db/seeders/users.js",
    "migrate": "npx sequelize db:migrate",
    "migrate:drop": "npx sequelize db:migrate:undo:all",
    "migrate:reset": "npx sequelize db:migrate:undo:all && npm run migrate",
    "start:dev": "nodemon --watch src --watch .env --watch package.json --watch .eslintrc src/index.js --exec babel-node --presets es2015,stage-2",
    "start": "npm run build && npm run migrate && node build/index.js",
    "restart": "npm run build && npm run migrate:reset && node build/index.js",
    "build": "babel ./src -d build/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "nodemon": "^1.19.0"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.0.2",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1"
  }
}
